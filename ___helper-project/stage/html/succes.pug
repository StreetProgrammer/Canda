- var pageTitle = 'succes'
<!DOCTYPE html>
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        meta(http-equiv="X-UA-Compatible", content="ie=edge")
        include pug/layout/styles
        title #{pageTitle}
    body 
        .page-wrapper.succes-page
            include pug/layout/sidebar
            include pug/layout/navbar
            main.page-content
                .container
                    .row
                        .col-md-4.offset-md-4.col-sm-12
                            .succes-container.white-bg.box-shadow
                                span.icon <i class="fas fa-check-circle"></i>
                                p تمت العملية بنجاح
                                    
            include pug/layout/footer           
        include pug/layout/js